    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Xun Zhou",
            "email": "xun.zhoup@mail.schwarz"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
